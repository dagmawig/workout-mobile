{
  "expo": {
    "name": "workouttracker",
    "slug": "workouttracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/home_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash_logo_f.png",
      "resizeMode": "contain",
      "backgroundColor": "#28547B"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.fitfusion.workout.tracker",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "./config_plugins/withAnimatedWebp"
    ],
    "extra": {
      "eas": {
        "projectId": "ac925d22-f79c-429b-af6b-5ade05f40e55"
      }
    }
  }
}
