{
  "expo": {
    "name": "FitFUSE",
    "slug": "FitFUSE",
    "version": "1.0.19",
    "orientation": "portrait",
    "icon": "./assets/home_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash_logo_f2.png",
      "resizeMode": "contain",
      "backgroundColor": "#28547B"
    },
    "notification": {
      "androidMode": "default",
      "icon": "./assets/noti_icon.png",
      "color": "#28547B"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.fitfuse.workout.tracker",
      "versionCode": 20,
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon_1.png",
        "backgroundColor": "#28547B"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "./config_plugins/withAnimatedWebp",
      "./android-datepicker-color-config-plugin.js",
      [
        "react-native-android-widget",
        {
          "widgets": [
            {
              "name": "Hello",
              "label": "Exercise Template",
              "minWidth": "320dp",
              "minHeight": "120dp",
              "description": "This is my first widget",
              "previewImage": "./assets/widget_prev.png",
              "updatePeriodMillis": 1800000
            }
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "42cddeca-4dae-4ba7-886c-615589c600e1"
      }
    }
  }
}